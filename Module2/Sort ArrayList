import java.util.ArrayList;
import java.util.Collections;

 class P7 {
    public static void main(String[] args) {
        
        
     ArrayList<String> list =new ArrayList<>();
	 
	 list.add("Kapil");
	 list.add("Dwayne");
	 list.add("Karan");
	 
	 Collections.reverse(list);
	 
	 System.out.println("List =" + list);
	 
	 
	 
	 
    }
}
