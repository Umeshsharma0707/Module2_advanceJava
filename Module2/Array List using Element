import java.util.ArrayList;

 class P7 {
    public static void main(String[] args) {
       
      ArrayList<String> list =new ArrayList<>();
	  
	  list.add("Kapil");
	  list.add("Dwayne");
	  list.add("Karan");
	  
	  
	 for(int i=0; i<list.size(); i++)
	 {
		 System.out.println("Position =" + i + ":" + list.get(i));
	 }
    }
}
