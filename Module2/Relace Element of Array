import java.util.ArrayList;

 class P7 {
    public static void main(String[] args) {
       
      ArrayList<String> list =new ArrayList<>();
	  
	  list.add("Kapil");
	  list.add("Dwayne");
	  list.add("Karan");
	  
	  
	 System.out.println("list =" + list);
	 
	 list.set(1," Rajiv");
	 
	 System.out.println("list =" + list);
    }
}
