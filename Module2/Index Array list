import java.util.ArrayList;

class P7
{
	public static void main(String args[])
	{
		ArrayList<String> list =new ArrayList<String>();
		
		
		list.add("Kapil");
		list.add("Dwayne");
		list.add("Karan");
		
		String element = list.get(0);
		
		System.out.println("Element at I Index " + element);
	}
}
