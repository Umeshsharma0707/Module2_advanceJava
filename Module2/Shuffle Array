import java.util.ArrayList;
import java.util.Collections;

class P7
{
	public static void main(String args[])
	{
		ArrayList<Integer> list =new ArrayList<>();
		
		
		list.add(3);
		list.add(7);
		list.add(8);
		list.add(5);
		
		Collections.shuffle(list);
		
		System.out.println("Arraylist =" + list);
	}
}
