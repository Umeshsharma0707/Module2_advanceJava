import java.util.ArrayList;

 class P7 {
    public static void main(String[] args) {
     
	 
	 ArrayList<Integer> list =new ArrayList<>();
	 
	 
	 list.add(2);
	 list.add(3);
	 list.add(7);
	 list.add(8);
	 list.add(5);
	 
	 System.out.println("list =" + list);
	 
	  ArrayList<Integer> list2 =new ArrayList<>();
	 
	 
	 list2.add(2);
	 list2.add(4);
	 list2.add(7);
	 list2.add(1);
	 list2.add(5);
	 
	 
	 list.retainAll(list2);
	 System.out.println("list =" + list);
	
    }
}
