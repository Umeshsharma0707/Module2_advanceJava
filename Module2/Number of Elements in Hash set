import java.util.HashSet;

 class HashSetSize {
    public static void main(String[] args) {
        
        HashSet<String> set = new HashSet<>();
        set.add("apple");
        set.add("banana");
        set.add("orange");

        
        int size = set.size();

        
        System.out.println("Number of elements in the HashSet: " + size);
    }
}
