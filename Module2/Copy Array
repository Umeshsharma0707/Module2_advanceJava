import java.util.ArrayList;


class P7
{
	public static void main(String args[])
	{
		ArrayList<String> list1 =new ArrayList<>();
		
		list1.add("Kapil");
		list1.add("Dwayne");
		list1.add("Karan");
		
		ArrayList<String> list2 =new ArrayList<>();
		
		list2.addAll(list1);
		
		System.out.println("list =" + list2);
	}
}
